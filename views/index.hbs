    <div class="carousel-inner">
      
      <div class="carousel-item active ">
      
        <img align="center" class="d-block size-slider" src="/images/slide/sm/slider_1.png" alt="First slide"/>
      </div>
      <div class="carousel-item">
        <img class="d-block size-slider" src="/images/slide/sm/slider_2.png" alt="Second slide">
      </div>
      <div class="carousel-item">
        <img class="d-block size-slider"  src="/images/slide/sm/slider_3.png" alt="Third slide">
      </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
